{
	"name": "pd/css-dependencies-version",
	"description": "Nástroj pro overzování závislostí v CSS souboru",
	"require": {
		"php": "7.1.* || 7.2.* || 7.3.* || 7.4.*",
		"symfony/console": "~2.3 || 3.* || 4.*",
		"nette/http": "3.0.*",
		"pd/version": ">= 3.0.0 <= 3.1.2",
		"psr/log": "^1.0.1",
		"kdyby/console": "2.8.*@dev"
	},
	"require-dev": {
		"nette/tester": ">= 2.3",
		"pd/coding-standard": "1.17.*",
		"phpstan/phpstan-strict-rules": "0.12.*"
	},
	"autoload": {
		"psr-4": {
			"Pd\\CssDependenciesVersion\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"PdTests\\CssDependenciesVersion\\": "tests/"
		}
	}
}
