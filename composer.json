{
	"name": "pd/css-dependencies-version",
	"description": "Nástroj pro overzování závislostí v CSS souboru",
	"require": {
		"php": "^7.4 || ^8.0",
		"symfony/console": "^3.0 | ^4.0 | ^5.0",
		"nette/http": "^3.0",
		"pd/version": "^3.3",
		"psr/log": "^1.0.1"
	},
	"require-dev": {
		"nette/tester": ">= 2.3",
		"pd/coding-standard": "1.27.*",
		"phpstan/phpstan-strict-rules": "0.12.*"
	},
	"suggest": {
		"kdyby/console": "For Symfony Console support",
		"contributte/console": "For Symfony Console support"
	},
	"autoload": {
		"psr-4": {
			"Pd\\CssDependenciesVersion\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"PdTests\\CssDependenciesVersion\\": "tests/"
		}
	}
}
